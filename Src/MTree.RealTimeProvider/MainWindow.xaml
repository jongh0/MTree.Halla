<Window x:Class="MTree.RealTimeProvider.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MTree.RealTimeProvider"
        mc:Ignorable="d"
        Title="RealTimeProvider" Height="350" Width="525">
    <Grid>
        <Button Content="Send log to email" Command="{Binding SendLogCommand}" IsEnabled="{Binding Path=CanExecuteSendLog}" Width="120" Height="30"/>
    </Grid>
</Window>
